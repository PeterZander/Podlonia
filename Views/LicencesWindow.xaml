<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="220"
        x:Class="Podlonia.Views.LicencesWindow"
        Width="400" Height="700"
        MinWidth="120" MinHeight="130"
        Icon="/Assets/RSS.ico"
        Title="Licences">
  <DockPanel>
    <TextBox
      TextAlignment="Left" BorderThickness="0"
      IsReadOnly="True" AcceptsReturn="True" TextWrapping="Wrap"
      Text="{Binding Text}" />
  </DockPanel>
</Window>
